#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jake/_code/kcdc-website-2023/node_modules/.pnpm/binode@1.0.5/node_modules/binode/node_modules:/Users/jake/_code/kcdc-website-2023/node_modules/.pnpm/binode@1.0.5/node_modules:/Users/jake/_code/kcdc-website-2023/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jake/_code/kcdc-website-2023/node_modules/.pnpm/binode@1.0.5/node_modules/binode/node_modules:/Users/jake/_code/kcdc-website-2023/node_modules/.pnpm/binode@1.0.5/node_modules:/Users/jake/_code/kcdc-website-2023/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../binode/bin.js" "$@"
else
  exec node  "$basedir/../binode/bin.js" "$@"
fi
